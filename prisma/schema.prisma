datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model GuestbookEntry {
  id         String   @id @default(cuid())
  email      String
  body       String
  created_by String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
